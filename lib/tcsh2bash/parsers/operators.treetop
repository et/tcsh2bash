module Tcsh2Bash
  grammar Operators

    rule operator
      eq / neq / lte / gte / lt / gt
    end

    rule eq
      '==' <Eq>
    end

    rule neq
      '!=' <Neq>
    end

    rule lt
      '<' <Lt>
    end

    rule lte
      '<=' <Lte>
    end

    rule gt
      '>' <Gt>
    end

    rule gte
      '>=' <Gte>
    end
  end
end
